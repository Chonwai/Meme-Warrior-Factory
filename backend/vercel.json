{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "requirementsPath": "requirements-minimal.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true",
    "VERCEL": "1",
    "SKIP_FILE_OPERATIONS": "1"
  }
} 