{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true"
  },
  "build": {
    "env": {
      "PIP_FLAGS": "--no-binary :all:"
    }
  }
} 